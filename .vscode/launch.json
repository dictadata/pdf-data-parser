  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "getContent",
      "program": "${workspaceFolder}/test/getContent.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "getCells",
      "program": "${workspaceFolder}/test/getCells.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node-terminal",
      "name": "---",
      "request": "launch"
    },
    {
      "type": "node",
      "name": "PdfDataParser",
      "program": "${workspaceFolder}/test/testDataParser.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "PdfDataReader",
      "program": "${workspaceFolder}/test/testDataReader.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "RowAsObjectTransform",
      "program": "${workspaceFolder}/test/testRowAsObject.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "RepeatCellTransform",
      "program": "${workspaceFolder}/test/testRepeatCell.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "RepeatHeadingTransform",
      "program": "${workspaceFolder}/test/testRepeatHeading.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node-terminal",
      "name": "---",
      "request": "launch"
    },
    {
      "type": "node",
      "name": "pdp helloworld json",
      "program": "pdf-data-parser.js",
      "args": [ ".\\test\\data\\pdf\\helloworld.pdf", "--headers=Greating" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "pdp helloworld csv",
      "program": "pdf-data-parser.js",
      "args": [ ".\\test\\data\\pdf\\helloworld.pdf", "--headers=Greating", "--format=csv" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "pdp helloworld rows",
      "program": "pdf-data-parser.js",
      "args": [ ".\\test\\data\\pdf\\helloworld.pdf", "--headers=Greating", "--format=rows" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "pdp ClassCodes csv",
      "program": "pdf-data-parser.js",
      "args": [ ".\\test\\data\\pdf\\ClassCodes.pdf", ".\\test\\output\\pdp\\ClassCodes.csv", "--format=csv" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "pdp ClassCodes json",
      "program": "pdf-data-parser.js",
      "args": [ ".\\test\\data\\pdf\\ClassCodes.pdf", ".\\test\\output\\pdp\\ClassCodes.json", "--format=json" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "pdp RepeatCell",
      "program": "pdf-data-parser.js",
      "args": [ "--options=.\\test\\optionsRepeatCell.json" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "pdp RepeatHeading",
      "program": "pdf-data-parser.js",
      "args": [ "--options=.\\test\\optionsRepeatHeading.json" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
