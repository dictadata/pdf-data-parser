  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "PdfDataParser",
      "cwd": "${workspaceFolder}/test/",
      "program": "${workspaceFolder}/test/testDataParser.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "PdfDataReader",
      "cwd": "${workspaceFolder}/test/",
      "program": "${workspaceFolder}/test/testDataReader.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "RowAsObjects",
      "cwd": "${workspaceFolder}/test/",
      "program": "${workspaceFolder}/test/testAsObjects.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "getContent",
      "cwd": "${workspaceFolder}/test/",
      "program": "${workspaceFolder}/test/getContent.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "name": "getCells",
      "cwd": "${workspaceFolder}/test/",
      "program": "${workspaceFolder}/test/getCells.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "cli helloworld.csv",
      "cwd": "${workspaceFolder}",
      "program": "pdf-data-parser.js",
      "args": [ ".\\test\\data\\pdf\\helloworld.pdf", "--headers=Greating" ],
      "outputCapture": "std",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "cli helloworld.json",
      "cwd": "${workspaceFolder}",
      "program": "pdf-data-parser.js",
      "args": [ ".\\test\\data\\pdf\\helloworld.pdf", ".\\test\\output\\cli\\helloworld.json", "--headers=Greating", "--json" ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "cli ClassCodes.csv",
      "cwd": "${workspaceFolder}",
      "program": "pdf-data-parser.js",
      "args": [ ".\\test\\data\\pdf\\ClassCodes.pdf", ".\\test\\output\\cli\\ClassCodes.csv", "--csv" ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "cli ClassCodes.json",
      "cwd": "${workspaceFolder}",
      "program": "pdf-data-parser.js",
      "args": [ ".\\test\\data\\pdf\\ClassCodes.pdf", ".\\test\\output\\cli\\ClassCodes.json", "--json" ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
